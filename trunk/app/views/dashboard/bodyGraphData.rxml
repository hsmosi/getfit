xml.instruct! :xml, :version=>"1.0"
xml.graph (:animation => "0", :bgColor => "FFDBDB", :showNames => "0", :decimalPrecision => "0", :formatNumberScale => "0", :yAxisMinValue => "#{@minValue}", :yAxisMaxValue => "#{@maxValue}", :showValues => "0") do
  @bodyMetrics.each do |b|
    xml.set(:value => b.weight)
  end
end